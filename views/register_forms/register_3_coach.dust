<!DOCTYPE html>
<html>
<body>
<form method="POST" action="/coaches" enctype="application/x-www-form-urlencoded">

    <input required name="name" type="text" placeholder="Name" ><!-- 0-->

    <input name="last-name" type="text" placeholder="Last Name" required><br><br><!--1-->

    <input name="username" type="text" placeholder="username" required><br><br> <!-- 2-->

    <input name="address-1" type="text" placeholder="Address 1" required> <br><br><!-- 3-->

    <input name="address-2" type="text" placeholder="Address 2" required><br><br><!-- 4-->
    <input type="email" name="email" required> <!-- 5-->

{!    {>"partials/nations"/} <!--0-->!}
    <br><br>

    <label for="photo">Set a profile pic:</label>
    <input type="file" name="photo" required>    <!-- 6-->

    <br><br>

    <label for="birthday">Birthday: </label>
    <input type="date" name="bday" required> <!--7-->

    <br><br>
    <label for="sex">Sex: </label>
    <select name="sex" class="form-control" required> <!-- 1-->
    <option value="Male">Male</option>
    <option value="Female">Female</option>
    <option value="Other">Other</option>
    </select><br><br>

    <label for="password">Password: </label>
    <input name="password" type="password" placeholder="pw" required><br><br><!-- 8-->

    <label for="phone">Phone Number: </label>
    <input type="tel" name="phone" placeholder="3884464115" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" required><br><br><!-- 9-->

    <label for="state">State: </label>
    <input type="text" name="city" placeholder="Lugano" required><br><br><!-- 10-->

    <label for="zipcode">zipCode: </label>
    <input type="text" name="zipCode" placeholder="6900" pattern="^\s*?\d{5}(?:[-\s]\d{4})?\s*?$" required><br><br> <!-- 11-->

    <label for="state">State: </label>
    <input type="text" name="State" placeholder="Canton Ticino" required><br><br><!--12-->

    <label for="currency">Currency: </label>
        <select id="currency" name="currency" required><!-- 2-->
            <option value="Euro">€</option>
            <option value="CHF">CHF</option>
            <option value="USD">$</option>
            <option value="AUS">AU$</option>
            <option value="CAN">$CAN</option>
            <option value="Yen">¥</option>
            <option value="GBP">£</option>
            <option value="SWE">kr</option>
        </select><br><br>
    
    <input type="submit" value="Create Account">
</form>


</body>

<script src="/js/resources/dustjs-linkedin/dust-core.min.js"></script>
<script src="/js/views.js"></script>
<script src="/js/fetch.js"></script>
<script src="/js/register.js"></script>

</html>
